{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "object-shorthand-properties-first": false,
    "member-access": false,
    "no-use-before-declare": false,
    "prefer-array-literal": false,
    "function-name": false
  }
}
